#El usuario debe ingresar un número
numero = int(input("Introduce un número: "))

#Imprimir la tabla de multiplicar del 1 al 10
print(f"Tabla de multiplicar del {numero}:")
for i in range(1, 11): #El rango se queda con el ultimo número
    print(f"{numero} x {i} = {numero * i}") #Imprime la tabla de multiplicar del número ingresado por el usuario
